  "controls": [
    {
      "name": "Billboard",
      "description": "Displays billboards based on progress messages."
    },
    {
      "name": "Bitmap",
      "description": "Displays a static picture of a bitmap."
    },
    {
      "name": "CheckBox",
      "description": "A two-state check box."
    },
    {
      "name": "ComboBox",
      "description": "A drop-down list with an edit field."
    },
    {
      "name": "DirectoryCombo",
      "description": "Select all except the last segment of the path."
    },
    {
      "name": "DirectoryList",
      "description": "Displays folders below the main part of path."
    },
    {
      "name": "Edit",
      "description": "A regular edit field for any string or integer."
    },
    {
      "name": "GroupBox",
      "description": "Displays a rectangle that groups other controls together."
    },
    {
      "name": "Hyperlink",
      "description": "Displays a HTML link to an address, which opens in the default browser.Windows Installer 4.5 and earlier: Not supported."
    },
    {
      "name": "Icon",
      "description": "Displays a static picture of an icon."
    },
    {
      "name": "Line",
      "description": "Displays a horizontal line."
    },
    {
      "name": "ListBox",
      "description": "A drop-down list without an edit field."
    },
    {
      "name": "ListView",
      "description": "Displays a column of values with icons for selection."
    },
    {
      "name": "MaskedEdit",
      "description": "An edit field with a mask in the text field."
    },
    {
      "name": "PathEdit",
      "description": "Displays folder name or entire path in an edit field."
    },
    {
      "name": "ProgressBar",
      "description": "Bar graph that changes length as it receives progress messages."
    },
    {
      "name": "PushButton",
      "description": "Displays a basic push button."
    },
    {
      "name": "RadioButtonGroup",
      "description": "A group of radio buttons."
    },
    {
      "name": "ScrollableText",
      "description": "Displays a long string of text."
    },
    {
      "name": "SelectionTree",
      "description": "Displays information from the Feature table and enables the user to change their selection state."
    },
    {
      "name": "Text",
      "description": "Displays static text."
    },
    {
      "name": "VolumeCostList",
      "description": "Displays costing information on different volumes."
    },
    {
      "name": "VolumeSelectCombo",
      "description": "Selects volume from an alphabetical list."
    }
